msgstr ""
"%type_name: Επεξεργαστείτε οποιοδήποτε "
"περιεχόμενο"
msgid "%type_name: Delete own content"
msgstr ""
"%type_name: Διαγράψτε το δικό σας "
"περιεχόμενο"
